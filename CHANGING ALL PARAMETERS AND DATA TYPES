
public class MethodOverloadingDemo {
     public void display() {
        System.out.println("No parameters");
    }

    public void display(int num) {
        System.out.println("One integer: " + num);
    }

    public void display(String message) {
        System.out.println("One string: " + message);
    }

    public void display(int num1, int num2) {
        System.out.println("Two integers: " + num1 + ", " + num2);
    }

    public void display(double num, String message) {
        System.out.println("One double and one string: " + num + ", " + message);
    }
    public static void main(String[] args) {
        MethodOverloadingDemo demo = new MethodOverloadingDemo();

        demo.display();

        demo.display(76);

        demo.display("PYTHON");

        demo.display(44,66);

        demo.display(9.98, "JAVA");
    }

   
}
