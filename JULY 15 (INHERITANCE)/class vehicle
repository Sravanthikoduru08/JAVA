class vehicle
{
    void drive() 
    {
        System.out.println("driving");
    }
}
class car extends vehicle
{
    void drive() 
    {
        System.out.println("repairing a car");
    }
}
public class Main 
{
    public static void main(String[] args) 
    {
        car c = new car();
        c.drive();  
    }
}
