public class MethodOverloadingDemo {
     public static double add(double a, double b) {
        return a + b;
    }
    public static String add(String a, String b) {
        return a + b;
    }

        public static double add(double a, double b, double c) {
        return a+b+c;
}
    public static void main(String[] args) {

        System.out.println("Calling add(double, double)");
        double result2 = add(4.67 ,9.7);
        System.out.println("Result: " + result2);

        System.out.println("Calling add(String, String)");
        String result3 = add("JAVA, ", "PYTHON!");
        System.out.println("Result: " + result3);
        System.out.println("Calling add(double, double, double)");
        double result5 = add(2.5, 3.7, 4.1);
        System.out.println("Result: " + result5);
    }
   
}
